{
    "name": "Linux App Lanucher",
    "version": "1.0.0",
    "description": "simple app lanucher for linux",
    "icon": "Icon.svg",
    "help": "README.md",
    "keyword": "app",
    "script": "main.py",
    "creator_name": "Osama Muhammed Alzabidi",
    "creator_email": "everyskils@gmail.com",
    "creator_url": "https://everyskills.com",
    "home_page": "https://github.com/everyskills/",
    "system": "linux",
    "settings": {
        "kw": {
            "type": "keyword",
            "value": "app",
            "title": "App Keyword"
        },

        "user_apps": {
            "type": "input",
            "value": "~/.local/share/applications",
            "title": "User Apps",
            "subtitle": "path to all user apps"
        },

        "sys_apps": {
            "type": "input",
            "value": "/usr/share/applications",
            "title": "System Apps",
            "subtitle": "path to all system apps"
        },

        "max_results": {
            "type": "num",
            "value": 12,
            "suffix": " item",
            "title": "Max Results"
        }
    },

    "enabled": true
}